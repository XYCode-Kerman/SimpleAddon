{
  "button_label": {
    "alpha": "$control_alpha",
    "color": "$label_color",
    "font_scale_factor": "$label_font_scale_factor",
    "font_size": "$label_font_size",
    "font_type": "smooth",
    "layer": "$label_layer",
    "max_size": ["100%", "100%"],
    "offset": "$label_offset",
    "shadow": false,
    "text": "$label_text",
    "text_alignment": "center",
    "type": "label"
  },
  "default": {
    "alpha": "$control_alpha",
    "is_new_nine_slice": "$is_new_nine_slice",
    "layer": "$texture_layer",
    "nine_slice_buttom": "$nine_slice_buttom",
    "nine_slice_left": "$nine_slice_left",
    "nine_slice_right": "$nine_slice_right",
    "nine_slice_top": "$nine_slice_top",
    "nineslice_size": "$nineslice_size",
    "texture": "$default_texture",
    "type": "image"
  },
  "hover": {
    "alpha": "$control_alpha",
    "is_new_nine_slice": "$is_new_nine_slice",
    "layer": "$texture_layer",
    "nine_slice_buttom": "$nine_slice_buttom",
    "nine_slice_left": "$nine_slice_left",
    "nine_slice_right": "$nine_slice_right",
    "nine_slice_top": "$nine_slice_top",
    "nineslice_size": "$nineslice_size",
    "texture": "$hover_texture",
    "type": "image"
  },
  "main": {
    "absorbs_input": true,
    "always_accepts_input": false,
    "controls": [
      {
        "source@redstone_weapon_hud_v2.source": {}
      },
      {
        "operation@redstone_weapon_hud_v2.operation": {}
      }
    ],
    "bindings": [
      {
        "binding_condition": "always",
        "binding_name": "#main.gametick"
      }
    ],
    "force_render_below": false,
    "is_showing_menu": true,
    "render_game_behind": true,
    "render_only_when_topmost": true,
    "should_steal_mouse": false,
    "type": "screen"
  },
  "namespace": "redstone_weapon_hud_v2",
  "operation": {
    "alpha": 1.0,
    "anchor_from": "center",
    "anchor_to": "center",
    "clip_offset": [0, 0],
    "clips_children": false,
    "enabled": true,
    "layer": 4,
    "max_size": [0, 0],
    "min_size": [0, 0],
    "offset": [0, 0],
    "propagate_alpha": false,
    "size": ["100%", "100%"],
    "type": "panel",
    "visible": true
  },
  "pressed": {
    "alpha": "$control_alpha",
    "is_new_nine_slice": "$is_new_nine_slice",
    "layer": "$texture_layer",
    "nine_slice_buttom": "$nine_slice_buttom",
    "nine_slice_left": "$nine_slice_left",
    "nine_slice_right": "$nine_slice_right",
    "nine_slice_top": "$nine_slice_top",
    "nineslice_size": "$nineslice_size",
    "texture": "$pressed_texture",
    "type": "image"
  },
  "source@common.button": {
    "$control_alpha": 1.0,
    "$default_texture": "textures/ui/common/blank",
    "$hover_texture": "textures/ui/common/blank",
    "$is_new_nine_slice": true,
    "$label_color": [1, 1, 1],
    "$label_font_scale_factor": 1.0,
    "$label_font_size": "normal",
    "$label_layer": 3,
    "$label_offset": [0, 0],
    "$label_text": "",
    "$nine_slice_buttom": 0,
    "$nine_slice_left": 0,
    "$nine_slice_right": 0,
    "$nine_slice_top": 0,
    "$nineslice_size": [0, 0, 0, 0],
    "$pressed_button_name": "%redstone_weapon_hud_v2.click",
    "$pressed_texture": "textures/ui/common/blank",
    "$texture_layer": 2,
    "alpha": 1.0,
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "bindings": [
      {
        "binding_collection_name": "",
        "binding_condition": "always_when_visible",
        "binding_type": "collection_details"
      }
    ],
    "button_mappings": [],
    "clip_offset": [0, 0],
    "clips_children": false,
    "controls": [
      {
        "default@redstone_weapon_hud_v2.default": {}
      },
      {
        "hover@redstone_weapon_hud_v2.hover": {}
      },
      {
        "pressed@redstone_weapon_hud_v2.pressed": {}
      },
      {
        "button_label@redstone_weapon_hud_v2.button_label": {}
      }
    ],
    "draggable": "not_draggable",
    "enabled": true,
    "is_handle_button_move_event": true,
    "layer": 0,
    "max_size": [0, 0],
    "min_size": [0, 0],
    "offset": [0, 0],
    "propagate_alpha": false,
    "size": [0.0, 0.0],
    "visible": true
  }
}
